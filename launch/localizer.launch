<?xml version="1.0"?>
<launch>

   <arg name="coordinate_system"       default="lanelet"  doc="{lest97, utm, lanelet}" />
   <arg name="use_msl_height"          default="False"     doc="msl - Mean Sea Level height or ellipsoid height." />

   <arg name="use_custom_origin"       default="True"     doc="does not influence lanelet projector - origin is used anyway!" />
   <node pkg="autoware_mini" type="localizer.py" name="gnss_localizer" output="screen" >
      <param name="coordinate_system"     value="$(arg coordinate_system)" />
      <param name="use_msl_height"        value="$(arg use_msl_height)" />
      <param name="use_custom_origin"     value="$(arg use_custom_origin)" />
   </node>


</launch>
